{
  "name": "jsonp-data",
  "version": "1.0.0",
  "description": "generate jsonp file by nodejs, read jsonp file by nodejs or brower",
  "main": "lib/index.js",
  "scripts": {
    "build:browser": "rimraf lib/jsonp-data.browser.js && babel browser/JsonpD.ts --out-file lib/jsonp-data.browser.js",
    "build:node": "tsc src/index.ts --outDir lib",
    "test:node": "npm run build:node && node examples/main.js",
    "test:browser": "npm run build:browser && http-server ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wenonly/jsonp-data.git"
  },
  "keywords": [
    "jsonp"
  ],
  "author": "wenonly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wenonly/jsonp-data/issues"
  },
  "homepage": "https://github.com/wenonly/jsonp-data#readme",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@types/node": "^20.1.4",
    "http-server": "^14.1.1",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-modules-umd": "^7.18.6",
    "@babel/preset-env": "^7.21.5"
  }
}
